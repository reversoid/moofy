{
  "name": "moofy-backend",
  "version": "1.1.3",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.12.19",
    "node": ">= 18.12.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "dev": "yarn start:dev",
    "start:dev": "docker compose --env-file ./config/.env -f docker-compose.dev.yml up -d && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "create-migration": "./create-migration.sh",
    "generate-migration": "./generate-migration.sh",
    "migration:run": "yarn typeorm migration:run -d dist/db/data-source.js",
    "migration:down": "yarn typeorm migration:revert -d dist/db/data-source.js",
    "migration:create": "yarn typeorm migration:create",
    "migration:generate": "yarn typeorm migration:generate -d dist/db/data-source.js"
  },
  "dependencies": {
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.2.8",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.8",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.8",
    "@nestjs/platform-socket.io": "^10.2.8",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.1.14",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.2.8",
    "@types/bcrypt": "^5.0.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "easy-yandex-s3": "^2.0.0",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "nestjs-rmq": "^2.12.0",
    "npm": "^10.2.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "sanitize-html": "^2.11.0",
    "sharp": "^0.32.6",
    "socket.io": "^4.7.2",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.8",
    "@types/axios": "^0.14.0",
    "@types/better-sqlite3": "^7.6.7",
    "@types/cookie-parser": "^1.4.6",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.7",
    "@types/lodash": "^4.14.200",
    "@types/multer": "^1.4.10",
    "@types/node": "^20.8.10",
    "@types/passport-jwt": "^3.0.13",
    "@types/sanitize-html": "^2.9.4",
    "@types/supertest": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "better-sqlite3": "^9.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "29.7.0",
    "prettier": "^3.0.3",
    "prisma": "^5.5.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    }
  }
}
